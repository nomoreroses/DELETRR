# Flask
SECRET_KEY=change-this-to-a-random-secret-key
FLASK_ENV=development
FLASK_DEBUG=1

# Database (SQLite par défaut, PostgreSQL en prod)
DATABASE_URL=sqlite:///rgpd_app.db

# Google OAuth
GOOGLE_CLIENT_SECRETS=client_secrets.json
OAUTH_REDIRECT_URI=http://localhost:5000/api/oauth/callback

# Frontend URL (pour redirection après OAuth)
FRONTEND_URL=http://localhost:5173
